{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Concepto</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ entity.nombre }}</td>
            </tr>
            <tr>
                <th>Valor</th>
                <td>{{ entity.valor }}</td>
            </tr>
            <tr>
                <th>Aplicaiva</th>
                <td>{{ entity.aplicaIva }}</td>
            </tr>
            <tr>
                <th>Ecuacion</th>
                <td>{{ entity.ecuacion }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('concepto') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('concepto_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
